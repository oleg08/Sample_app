<% if logged_in? %>
    <div class="row">
      <%= render 'shared/if_logged_in' %>
    </div>
<% else %>
    <div class="center jumbotron">
      <%= render 'shared/if_not_logged_in' %>
    </div>

    <%= link_to image_tag("rails.png", alt: "Rails logo"),
                'http://rubyonrails.org/' %>
<% end %>